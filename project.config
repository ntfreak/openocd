[project]
	description = Open On-Chip Debugger
[receive]
	requireChangeId = true
	requireSignedOffBy = true
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group Code Review
	label-Verified = -1..+1 group Code Review
	submit = group Code Review
[access "refs/tags/*"]
	push = group Release Manager
[submit]
	action = cherry pick
	mergeContent = true
